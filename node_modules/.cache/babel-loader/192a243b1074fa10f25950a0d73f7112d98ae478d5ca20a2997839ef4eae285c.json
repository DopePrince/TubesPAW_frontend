atus Pembayaran\"\r\n                            required>\r\n                        </v-text-field> -->\r\n\r\n                        <v-select\r\n                        v-model=\"formTodo.service\"\r\n                        :items=\"services\"\r\n                        item-text=\"nama_service\"\r\n                        item-value=\"id\"\r\n                        label=\"Nama Service\"\r\n                        persistent-hint\r\n                        return-object\r\n                        single-line\r\n                        ></v-select>\r\n                    </v-container>\r\n                </v-card-text>\r\n\r\n                <v-card-actions>\r\n                    <v-spacer></v-spacer>\r\n                    <v-btn color=\"blue darken-1\" text @click=\"cancel\">Cancel</v-btn>\r\n                    <v-btn v-if=\"formTodo.id_update==0\" color=\"blue darken-1\" text @click=\"addOrderServices\">Save</v-btn>\r\n                    <v-btn v-else color=\"blue darken-1\" text @click=\"updateOrderServices\">Save</v-btn>\r\n                </v-card-actions>\r\n            </v-card>\r\n        </v-dialog>\r\n    </v-main>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n    name: \"ListItem\",\r\n    data(){\r\n        return {\r\n            formTodo : {\r\n                booking: 0,\r\n                service : undefined,\r\n                status_pembayaran: undefined,\r\n                id_update: 0\r\n            },\r\n            bookings : [],\r\n            services : [],\r\n            search: null,\r\n            dialog: false,\r\n            headers: [\r\n                {\r\n                    text: \"No Booking\",\r\n                    align: \"start\",\r\n                    sortable: true,\r\n                    value: \"id_booking\",\r\n                },\r\n                { text: \"Status Pembayaran\", value: \"status_pembayaran\" },\r\n                { text: \"Nama Service\", value: \"id_service\" }